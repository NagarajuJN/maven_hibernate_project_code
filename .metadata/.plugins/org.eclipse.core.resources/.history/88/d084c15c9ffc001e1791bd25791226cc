/// Encapsulation Example

class Bank{
	private int acc_balance ;
	
	public int getBal() {
		return acc_balance;
	}
	public void setBal(int amount , int new_deposit) {
		acc_balance = amount;
	}
	}

public class Encapsulation {
	public static void main(String[] args)
	{
		Bank b = new Bank();
		b.setBal(10000,12);
			
		System.out.println(b.getBal());
		}

	}
